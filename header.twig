<header class="template-header container">
	<div class="row">
		<div class="header-title span-12">
			{{widget('logo', 'logo', {"defaultlogo": asset('images/logo.svg')})|raw}}
			{{widget('companyname', 'companyname', {"business": "Lorem Ipsum"})|raw}}
		</div>
		<nav class="template-nav span-12">
			{{widget('extendednavigation', 'navigationheader', {"align":"left"})|raw}}
			{% if page.request != 'checkout' %}
				{{widget('ecombasket', 'ecombasket', {})|raw}}
			{% endif %}
		</nav>
	</div>
</header>